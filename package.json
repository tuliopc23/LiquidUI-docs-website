{
  "name": "liquidify-docs-website",
  "version": "1.1.0",
  "private": true,
  "description": "Documentation site for Liquidify component library",
  "homepage": "https://liquidify-docs.vercel.app",
  "repository": {
    "type": "git",
    "url": "https://github.com/tuliopc23/Liquidify-docs.git"
  },
  "bugs": {
    "url": "https://github.com/tuliopc23/Liquidify-docs/issues"
  },
  "main": "index.js",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --ci --coverage --watchAll=false",
    "build:analyze": "ANALYZE=true next build",
    "lighthouse": "lhci autorun",
    "a11y-test": "pa11y-ci --sitemap http://localhost:3000/sitemap.xml",
    "type-check": "tsc --noEmit",
    "test:e2e": "playwright test",
    "test:all": "npm run type-check && npm run lint && npm run test:ci"
  },
  "keywords": [
    "liquidify",
    "component-library",
    "documentation",
    "react",
    "ui",
    "glassmorphism"
  ],
  "author": "Tulio Pinheiro Cunha",
  "license": "MIT",
  "dependencies": {
    "@emotion/is-prop-valid": "^1.3.1",
    "@ibm-cloud/watsonx-ai": "^1.6.8",
    "@modelcontextprotocol/sdk": "^1.13.2",
    "@smithery/sdk": "^1.5.2",
    "framer-motion": "^11.0.0",
    "liquidify": "^1.0.21",
    "lucide-react": "^0.400.0",
    "next": "^15.3.4",
    "next-themes": "^0.4.6",
    "nextra": "^2.13.4",
    "nextra-theme-docs": "^2.13.4",
    "react": "^18.3.0",
    "react-dom": "^18.3.0",
    "remark-emoji": "^4.0.1",
    "web-vitals": "^5.0.3"
  },
  "devDependencies": {
    "@lhci/cli": "^0.15.1",
    "@next/bundle-analyzer": "^14.2.30",
    "@playwright/test": "^1.40.0",
    "@tailwindcss/typography": "^0.5.10",
    "@testing-library/jest-dom": "^6.1.0",
    "@testing-library/react": "^14.1.0",
    "@testing-library/user-event": "^14.5.0",
    "@types/jest": "^29.5.0",
    "@types/node": "^20.0.0",
    "@types/react": "^18.3.0",
    "@types/react-dom": "^18.3.0",
    "autoprefixer": "^10.4.0",
    "eslint": "8.57.1",
    "eslint-config-next": "15.3.4",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "lighthouse": "^12.6.1",
    "pa11y-ci": "^2.4.2",
    "postcss": "^8.4.0",
    "tailwindcss": "^3.4.0",
    "typescript": "^5.0.0"
  }
}
