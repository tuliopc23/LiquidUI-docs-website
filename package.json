{
  "name": "liquidify-docs-website",
  "version": "2.0.0",
  "type": "module",
  "private": true,
  "description": "Documentation site for Liquidify component library - Modern Nextra v4 + App Router",
  "homepage": "https://liquidify-docs.vercel.app",
  "repository": {
    "type": "git",
    "url": "https://github.com/tuliopc23/Liquidify-docs.git"
  },
  "bugs": {
    "url": "https://github.com/tuliopc23/Liquidify-docs/issues"
  },
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --ci --coverage --watchAll=false",
    "build:analyze": "ANALYZE=true next build",
    "lighthouse": "lhci autorun",
    "a11y-test": "pa11y-ci --sitemap http://localhost:3000/sitemap.xml",
    "type-check": "tsc --noEmit",
    "test:e2e": "playwright test",
    "test:all": "npm run type-check && npm run lint && npm run test:ci",
    "postinstall": "node -e \"try { require('@napi-rs/simple-git') } catch (e) { console.log('simple-git optional dependency not available, continuing...') }\""
  },
  "keywords": [
    "liquidify",
    "component-library",
    "documentation",
    "react",
    "ui",
    "glassmorphism",
    "nextra",
    "app-router"
  ],
  "author": "Tulio Pinheiro Cunha",
  "license": "MIT",
  "dependencies": {
    "@emotion/is-prop-valid": "^1.3.1",
    "@heroicons/react": "^2.2.0",
    "@ibm-cloud/watsonx-ai": "^1.6.8",
    "@modelcontextprotocol/sdk": "^1.15.0",
    "@sentry/nextjs": "^9.35.0",
    "@sentry/node": "^9.35.0",
    "@sentry/profiling-node": "^9.35.0",
    "@smithery/sdk": "^1.5.2",
    "clsx": "^2.1.1",
    "framer-motion": "^12.23.0",
    "gsap": "^3.13.0",
    "liquidify": "^1.0.22",
    "lucide-react": "^0.525.0",
    "next": "^15.3.5",
    "next-themes": "^0.4.6",
    "nextra": "^5.0.0-alpha.24",
    "nextra-theme-docs": "^5.0.0-alpha.24",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "remark-emoji": "^5.0.1",
    "tailwind-merge": "^3.3.1",
    "web-vitals": "^5.0.3"
  },
  "optionalDependencies": {
    "@napi-rs/simple-git-linux-x64-gnu": "^0.1.19",
    "@napi-rs/simple-git-linux-x64-musl": "^0.1.19",
    "@napi-rs/simple-git-linux-arm64-gnu": "^0.1.19",
    "@napi-rs/simple-git-linux-arm64-musl": "^0.1.19"
  },
  "devDependencies": {
    "@lhci/cli": "^0.15.1",
    "@next/bundle-analyzer": "^15.3.5",
    "@playwright/test": "^1.48.1",
    "@tailwindcss/postcss": "^4.1.11",
    "@tailwindcss/typography": "^0.5.16",
    "@testing-library/jest-dom": "^6.1.0",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.5.0",
    "@types/jest": "^30.0.0",
    "@types/node": "^24.0.10",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "autoprefixer": "^10.4.0",
    "eslint": "^8.57.1",
    "eslint-config-next": "^15.3.5",
    "jest": "^30.0.4",
    "jest-environment-jsdom": "^30.0.4",
    "lighthouse": "^12.7.1",
    "pa11y-ci": "^3.1.0",
    "postcss": "^8.4.0",
    "stylelint": "^16.21.1",
    "stylelint-config-standard": "^38.0.0",
    "tailwindcss": "^4.1.11",
    "typescript": "^5.0.0"
  }
}
